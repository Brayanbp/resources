# ruta general: ~/.gitconfig
# ruta general: home/{nombre-de-usuario}/.gitconfig

[user]
	name = Jan Sanchez
	email = joejansanchez@gmail.com
[credential]
	helper = cache --timeout=3600
[alias]
	st = status 
	pu = pull 
	ck = checkout 
	cb = checkout -b 
	br = branch 
	cm = commit 
	ca = commit -am 
	ad = add .
	me = merge 
	ps = push 
	or = origin
	db= branch -d 
	lg = log --graph -10
	ll = log --graph -30 --pretty=format:'%Cgreen%h %Creset%ad %Cblue%an %Creset%s'
	in = fetch --dry-run
	out = push --dry-run
[core]
	editor = vim